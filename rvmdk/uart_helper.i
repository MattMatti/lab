    while(!SysCtlPeripheralReady(0xf0000805))
   {
    
  }
    
  
    
    GPIOPinTypeGPIOOutput(0x40025000, 0x00000008);
  
  
   
   
	 {
    UARTCharPut(0x4000C000, temp);  
